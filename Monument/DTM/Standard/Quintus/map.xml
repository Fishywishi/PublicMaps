<map proto="1.5.0">
<name>Quintus</name>
<version>1.2.0</version>
<include id="gapple-kill-reward"/>
<objective>Destroy the enemy team's monuments!</objective>
<gamemode>dtm</gamemode>
<authors>
    <author uuid="3893a242-cc17-447e-9b1c-774fc8941af8"/> <!-- G_SKIPPY -->
</authors>
<teams>
    <team id="cyan-team" color="dark aqua" max="40">Cyan</team>
    <team id="purple-team" color="dark purple" max="40">Purple</team>
</teams>
<!-- Kits -->
<kits>
    <kit id="spawn-kit" force="true">
        <clear/>
        <item slot="0" unbreakable="true">iron sword</item>
        <item slot="1" unbreakable="true" enchantment="arrow infinite:1">bow</item>
        <item slot="2" unbreakable="true" enchantment="dig speed:1">diamond pickaxe</item>
        <item slot="3" unbreakable="true">stone axe</item>
        <item slot="4" amount="2">golden apple</item>
        <item slot="5" amount="64">glass</item>
        <item slot="6" amount="32">log</item>
        <item slot="28" amount="1">arrow</item>
        <helmet unbreakable="true" team-color="true">leather helmet</helmet>
        <chestplate unbreakable="true" team-color="true">leather chestplate</chestplate>
        <leggings unbreakable="true" team-color="true">leather leggings</leggings>
        <boots unbreakable="true" team-color="true">leather boots</boots>
        <effect duration="5" amplifier="5">damage resistance</effect>
    </kit>
</kits>
<!-- Spawns -->
<spawns>
    <spawn team="cyan-team" kit="spawn-kit" yaw="0">
        <region>
            <cylinder base="40.5,34,-902.5" height="0" radius="1"/>
        </region>
    </spawn>
    <spawn team="purple-team" kit="spawn-kit" yaw="180">
        <region>
            <cylinder base="40.5,34,-776.5" height="0" radius="1"/>
        </region>
    </spawn>
    <default yaw="-90">
        <region>
            <cylinder base="-30.5,90,-839.5" height="0" radius="1"/>
        </region>
    </default>
</spawns>
<!-- Regions -->
<regions>
    <union id="deny-build">
        <cylinder base="40,32,-777" height="6" radius="9"/>
        <!-- Purple -->
        <cylinder base="40,32,-903" height="6" radius="9"/>
        <!-- Cyan -->
        <cuboid min="39,32,-783" max="42,37,-813"/>
        <cuboid min="39,32,-895" max="42,37,-868"/>
        <cuboid min="48,32,-881" max="33,37,-876"/>
        <cuboid min="48,32,-798" max="33,37,-803"/>
    </union>
    <!-- Purple Monuments -->
    <block id="purple-front-left" location="73,37,-777"/>
    <block id="purple-front-right" location="7,37,-777"/>
    <block id="purple-back-left" location="73,37,-744"/>
    <block id="purple-back-right" location="7,37,-744"/>
    <block id="purple-back-middle" location="40,37,-744"/>
    <!-- Cyan Monuments -->
    <block id="cyan-front-left" location="7,37,-903"/>
    <block id="cyan-front-right" location="73,37,-903"/>
    <block id="cyan-back-left" location="7,37,-936"/>
    <block id="cyan-back-right" location="73,37,-936"/>
    <block id="cyan-back-middle" location="40,37,-936"/>
    <apply region="deny-build" block="never" message="You may not build here!"/>
</regions>
<!-- Monuments -->
<!-- Purple Team -->
<destroyables owner="purple-team" repairable="false">
     <destroyable name="Front Left" region="purple-front-left" materials="obsidian"/>
     <destroyable name="Front Right" region="purple-front-right" materials="obsidian"/>
     <destroyable name="Back Left" region="purple-back-left" materials="obsidian"/>
     <destroyable name="Back Right" region="purple-back-right" materials="obsidian"/>
     <destroyable name="Back Middle" region="purple-back-middle" materials="obsidian"/>
</destroyables>
<!-- Cyan Team -->
<destroyables owner="cyan-team" repairable="false">
     <destroyable name="Front Left" region="cyan-front-left" materials="obsidian"/>
     <destroyable name="Front Right" region="cyan-front-right" materials="obsidian"/>
     <destroyable name="Back Left" region="cyan-back-left" materials="obsidian"/>
     <destroyable name="Back Right" region="cyan-back-right" materials="obsidian"/>
     <destroyable name="Back Middle" region="cyan-back-middle" materials="obsidian"/>
</destroyables>
<!-- Tool Repair -->
<toolrepair>
    <tool>iron sword</tool>
    <tool>bow</tool>
    <tool>diamond pickaxe</tool>
    <tool>stone axe</tool>
</toolrepair>
<!-- Item Keep -->
<itemkeep>
    <item>arrow</item>
    <item>glass</item>
    <item>log</item>
    <item>wood</item>
    <item>golden apple</item>
</itemkeep>
<!-- Item Remove -->
<itemremove>
    <item>obsidian</item>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>leather leggings</item>
    <item>leather boots</item>
</itemremove>
<!-- Misc -->
<hunger>
    <depletion>off</depletion>
</hunger>
<timelock>on</timelock>
</map>