<map proto="1.5.0">
<name>Titan</name>
<version>1.2.0</version>
<include id="gapple-kill-reward"/>
<objective>Control all 5 hills!</objective>
<gamemode>koth</gamemode>
<authors>
    <author uuid="459fd919-0d58-401a-9601-6cab1eb721ab" contribution="Design, gear system, XML, terraforming, and building"/> <!-- Reshif -->
    <author uuid="c33a03bf-0f73-46e2-a93e-ddb7d25232d1" contribution="Design and building"/> <!-- teheggy -->
</authors>
<contributors>
    <contributor uuid="15dc7f7a-40cd-4389-ad45-f8a33a09a394" contribution="Building help"/> <!-- moo88888888 -->
    <contributor uuid="22ba442b-cf26-4d09-b290-8d4e50a551b2" contribution="Building help"/> <!-- dinner1111 -->
    <contributor uuid="621b2aa8-4e5f-4ed8-bd36-0ae2f8952a06" contribution="Building help"/> <!-- thestrangeboy1 -->
</contributors>
<rules>
    <rule>Fall damage is disabled!</rule>
</rules>
<teams>
    <team id="red-team" color="dark red" max="40">Red</team>
    <team id="blue-team" color="blue" max="40">Blue</team>
</teams>
<!-- Kits -->
<kits>
    <kit id="spawn-kit" force="true">
        <clear/>
        <item unbreakable="true" slot="0">iron sword</item>
        <item unbreakable="true" slot="1" enchantment="arrow infinite:1;arrow damage:2">bow</item>
        <item slot="28" amount="1">arrow</item>
        <item slot="2">golden apple</item>
        <helmet enchantment="protection projectile:1" unbreakable="true">iron helmet</helmet>
        <chestplate enchantment="protection projectile:1" unbreakable="true">iron chestplate</chestplate>
        <leggings unbreakable="true" team-color="true">leather leggings</leggings>
        <boots unbreakable="true">iron boots</boots>
        <effect duration="5">heal</effect>
        <effect duration="10">damage resistance</effect>
    </kit>
</kits>
<!-- Spawns -->
<spawns>
    <spawn team="red-team" kit="spawn-kit" region="red-spawn"/>
    <spawn team="blue-team" kit="spawn-kit" yaw="180" region="red-spawn"/>
    <default yaw="90" region="obs-spawn"/>
</spawns>
<!-- Portals -->
<portals>
    <portal x="0" y="56" z="0" yaw="180">
        <region>
            <cuboid min="-2,8,112" max="5,12,113"/>
        </region>
    </portal>
    <portal x="0" y="56" z="0" yaw="180">
        <region>
            <cuboid min="-2,7,-114" max="3,10,-113"/>
        </region>
    </portal>
</portals>
<!-- Control Points -->
<control-points capture-time="8s" neutral-state="true" show-progress="true" incremental="true">
    <!-- Quarry -->
    <control-point name="Quarry">
        <capture>
            <region id="quarry"/>
        </capture>
        <progress>
            <region id="quarry"/>
        </progress>
        <captured>
            <region id="quarry-captured"/>
        </captured>
    </control-point>
    <!-- Radio Station -->
    <control-point name="Radio Station">
        <capture>
            <region id="radio-station"/>
        </capture>
        <progress>
            <region id="radio-progress"/>
        </progress>
        <captured>
            <region id="radio-captured"/>
        </captured>
    </control-point>
    <!-- Missile Silo -->
    <control-point name="Missile Silo">
        <capture>
            <region id="missile-silo"/>
        </capture>
        <progress>
            <region id="missile-progress"/>
        </progress>
        <captured>
            <region id="missile-captured"/>
        </captured>
    </control-point>
    <!-- Ruins -->
    <control-point name="Ruins">
        <capture>
            <region id="ruins"/>
        </capture>
        <progress>
            <region id="ruins-progress"/>
        </progress>
        <captured>
            <region id="ruins-captured"/>
        </captured>
    </control-point>
    <!-- Crateyard -->
    <control-point name="Crateyard">
        <capture>
            <region id="crateyard"/>
        </capture>
        <progress>
            <region id="crateyard-progress"/>
        </progress>
        <captured>
            <region id="crateyard-captured"/>
        </captured>
    </control-point>
</control-points>
<!-- Score Limit -->
<score>
    <limit>2500</limit>
</score>
<!-- Filters -->
<filters>
    <all id="allow-some">
        <any>
            <material>tnt</material>
            <material>web</material>
        </any>
    </all>
</filters>
<regions>
    <!-- Definitions -->
    <cuboid id="red-spawn-protection" min="-2,7,-114" max="3,10,-113"/>
    <cuboid id="blue-spawn-protection" min="-2,8,112" max="5,12,113"/>
    <rectangle id="playable" min="-oo,-oo" max="oo,oo"/>
    <cylinder id="red-spawn" base="0.5,64,-112" radius="3" height="0"/>
    <cylinder id="blue-spawn" base="1.5,64,108.5" radius="3" height="0"/>
    <cylinder id="obs-spawn" base="0.5,60,0.5" radius="3" height="0"/>
    <!-- Control Points -->
    <!-- Quarry -->
    <cuboid id="quarry" min="-6,2,-7" max="9,7,8"/>
    <cuboid id="quarry-captured" min="-6,2,-7" max="9,24,8"/>
    <!-- Radio Station -->
    <cuboid id="radio-station" min="57,7,-51" max="66,11,-42"/>
    <cuboid id="radio-progress" min="55,6,-53" max="68,11,-40"/>
    <cuboid id="radio-captured" min="53,6,-55" max="70,36,-38"/>
    <!-- Missile Silo -->
    <cuboid id="missile-silo" min="-61,6,37" max="-46,9,52"/>
    <cuboid id="missile-progress" min="-62,6,36" max="-45,9,53"/>
    <cuboid id="missile-captured" min="-65,6,33" max="-42,36,56"/>
    <!-- Ruins -->
    <cuboid id="ruins" min="-105,6,-14" max="-95,10,-4"/>
    <cuboid id="ruins-progress" min="-110,6,-19" max="-90,14,1"/>
    <cuboid id="ruins-captured" min="-110,6,-19" max="-90,20,1"/>
    <!-- Crateyard -->
    <cuboid id="crateyard" min="93,6,4" max="107,10,17"/>
    <cuboid id="crateyard-progress" min="93,6,4" max="107,10,17"/>
    <cuboid id="crateyard-captured" min="93,6,4" max="107,20,17"/>
    <!-- Applications -->
    <apply region="blue-spawn-protection" enter="blue-team" message="You may not enter Blue's spawn!"/>
    <apply region="red-spawn-protection" enter="red-team" message="You may not enter Red's spawn!"/>
    <apply region="playable" block="allow-some"/>
</regions>
<!-- Tool Repair -->
<toolrepair>
    <tool>iron sword</tool>
    <tool>bow</tool>
    <tool>stone sword</tool>
    <tool>wood sword</tool>
    <tool>gold sword</tool>
</toolrepair>
<!-- Item Remove -->
<itemremove>
    <item>iron helmet</item>
    <item>iron chestplate</item>
    <item>leather leggings</item>
    <item>iron boots</item>
    <item>glass bottle</item>
    <item>string</item>
</itemremove>
<!-- Item Keep -->
<itemkeep>
    <item>arrow</item>
    <item>gold ingot</item>
    <item>potion</item>
    <item>golden apple</item>
    <item>tnt</item>
    <item>web</item>
    <item>ender pearl</item>
    <item>monster egg</item>
</itemkeep>
<!-- Kill Rewards -->
<kill-rewards>
    <kill-reward>
        <item amount="4">gold ingot</item>
    </kill-reward>
</kill-rewards>
<!-- Misc -->
<tnt>
    <instantignite>on</instantignite>
    <licensing>off</licensing>
    <friendly-defuse>off</friendly-defuse>
    <blockdamage>off</blockdamage>
</tnt>
<mobs>
    <filter>
        <all>
            <mob>creeper</mob>
            <spawn>spawner egg</spawn>
            <spawn>spawner</spawn>
        </all>
    </filter>
</mobs>
<disabledamage>
    <damage>fall</damage>
</disabledamage>
<hunger>
    <depletion>off</depletion>
</hunger>
</map>