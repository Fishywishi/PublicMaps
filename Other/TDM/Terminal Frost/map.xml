<map proto="1.5.0">
<name>Terminal Frost</name>
<version>1.1.0</version>
<include id="gapple-kill-reward"/>
<time result="score">10m</time>
<score>
    <kills>1</kills>
</score>
<objective>Have the most kills before time runs out!</objective>
<gamemode>tdm</gamemode>
<authors>
    <author uuid="d341db34-f1ab-406e-b290-da6a6703565e"/> <!--CheetoPuf-->
    <author uuid="b0b3c5d5-e9bd-4984-bc07-1bd552d4b042"/> <!--Beebou-->
</authors>
<teams>
    <team id="cyan-team" color="dark aqua" max="32" max-overfill="36">Cyan</team>
    <team id="orange-team" color="gold" max="32" max-overfill="36">Orange</team>
</teams>
<kits>
    <kit id="spawn-kit" force="true">
        <clear/>
        <item unbreakable="true" slot="0" material="stone sword"/>
        <item unbreakable="true" slot="1" enchantment="arrow infinite" material="bow"/>
        <item slot="8" team-color="true" material="wool"/>
        <item slot="28" material="arrow"/>
        <helmet unbreakable="true" team-color="true" locked="true" material="leather helmet"/>
        <chestplate unbreakable="true" team-color="true" locked="true" enchantment="protection projectile:2" material="leather chestplate"/>
        <leggings unbreakable="true" team-color="true" locked="true" material="leather leggings"/>
        <boots unbreakable="true" team-color="true" locked="true" material="leather boots"/>
    </kit>
</kits>
<spawns>
    <spawn team="cyan-team" kit="spawn-kit">
        <region yaw="90">
            <cuboid min="86,10,5" max="91,10,10"/>
        </region>
    </spawn>
    <spawn team="orange-team" kit="spawn-kit">
        <region yaw="-90">
            <cuboid min="-86,10,5" max="-81,10,10"/>
        </region>
    </spawn>
    <default>
        <regions>
            <point>4.5,39,0.5</point>
            <point>0.5,39,0.5</point>
        </regions>
    </default>
</spawns>
<regions>
    <apply block="never"/>
</regions>
<toolrepair>
    <tool>stone sword</tool>
    <tool>bow</tool>
</toolrepair>
<itemkeep>
    <item>arrow</item>
    <item>golden apple</item>
</itemkeep>
<itemremove>
    <item>wool</item>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>leather leggings</item>
    <item>leather boots</item>
</itemremove>
<hunger>
    <depletion>off</depletion>
</hunger>
</map>