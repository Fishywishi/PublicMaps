<map proto="1.5.0">
<name>Fortress</name>
<version>1.0.1</version>
<objective>Eliminate the opposing team.</objective>
<gamemode>blitz</gamemode>
<gamemode>arcade</gamemode>
<authors>
    <author uuid="2c1cf65f-f668-41eb-812e-091391eb2896"/> <!-- Loks -->
</authors>

<teams>
   <team id="red-team" color="dark red" max="3" max-overfill="5">Red</team>
   <team id="blue-team" color="blue" max="3" max-overfill="5">Blue</team>
</teams>
<kits>
    <kit id="spawn-kit" force="true">
        <item slot="0" unbreakable="true" material="stone sword"/>
        <item slot="1" unbreakable="true" material="bow"/>
        <item slot="2" amount="12" material="wood"/>
        <item slot="3" material="golden apple"/>
        <item slot="9" amount="8" material="arrow"/>
        <helmet unbreakable="true" team-color="true" material="leather helmet"/>
        <chestplate unbreakable="true" team-color="true" material="leather chestplate"/>
        <leggings unbreakable="true" material="chainmail leggings"/>
        <boots unbreakable="true" team-color="true" material="leather boots"/>
    </kit>
</kits>
<spawns>
    <spawn team="red-team" kit="spawn-kit" yaw="-90">
        <region>
            <point>-17,4,11</point>
        </region>
    </spawn>
    <spawn team="blue-team" kit="spawn-kit" yaw="-90">
        <region>
            <point>-17,4,61</point>
        </region>
    </spawn>
    <default yaw="-90">
        <region>
            <point>-18.5,17,36.5</point>
        </region>
    </default>
</spawns>
<regions>
    <negative id="not-map">
        <rectangle id="map" min="-25,0" max="6,-73"/>
    </negative>
    <apply region="not-map" block="never" message="You may not edit outside the map!"/>
</regions>
<blitz>
    <lives>1</lives>
    <broadcastLives>false</broadcastLives>
</blitz>
<itemremove>
    <item>stone sword</item>
    <item>bow</item>
    <item>wood</item>
    <item>cooked beef</item>
    <item>arrow</item>
    <item>golden apple</item>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>chainmail leggings</item>
    <item>leather boots</item>
</itemremove>
<maxbuildheight>11</maxbuildheight>
<hunger>
    <depletion>off</depletion>
</hunger>
</map>
