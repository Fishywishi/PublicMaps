<map proto="1.5.0" game="Blitz: Sumo">
<name>Kilo Sumo</name>
<created>2021-10-30</created>
<version>2.0.2</version>
<objective>Be the last team standing by throwing everyone off the platform!</objective>
<gamemode>arcade</gamemode>
<gamemode>blitz</gamemode>
<!-- Authors & Contributors -->
<authors>
    <author uuid="571c1d9d-4d49-43bd-bfdb-30563e920ca9"/> <!-- Wesdial -->
    <author uuid="f3a45bea-dbde-4791-8155-0988b644ac3b"/> <!-- _iMan -->
</authors>
<contributors>
    <contributor uuid="f846db63-3df5-4e81-9dd3-7bb05027fdfb" contribution="JSON to XML conversion"/> <!-- TheBestGamer -->
</contributors>
<!-- Teams -->
<teams>
    <team id="red" color="dark red" min="1" max="24">Red</team>
    <team id="blue" color="blue" min="1" max="24">Blue</team>
</teams>
<!-- Time Limit & Lives -->
<time>3m</time>
<blitz>
    <lives>1</lives>
</blitz>
<!-- Broadcasts -->
<broadcasts>
    <alert after="98s">`cWatch out! The world border is getting close!</alert>
</broadcasts>
<!-- Kits -->
<kits>
    <!-- Red Kit -->
    <kit id="red-kit">
        <game-mode>adventure</game-mode>
        <item slot="0" name="`cSUMO WEAPON" damage="1" material="ink sack"/>
        <helmet unbreakable="true" locked="true" team-color="true" material="leather helmet"/>
        <effect duration="oo" amplifier="2">instant_health</effect>
    </kit>
    <!-- Blue Kit -->
    <kit id="blue-kit">
        <game-mode>adventure</game-mode>
        <item slot="0" name="`9SUMO WEAPON" damage="4" material="ink sack"/>
        <helmet unbreakable="true" locked="true" team-color="true" material="leather helmet"/>
        <effect duration="oo" amplifier="2">instant_health</effect>
    </kit>
    <!-- Void Kit -->
    <kit id="void-kit" force="true">
        <clear items="false" armor="false" effects="true"/>
    </kit>
</kits>
<!-- Spawns -->
<spawns>
    <!-- Red Team -->
    <spawn team="red" kit="red-kit">
        <regions yaw="-90">
            <cuboid min="-9,84,-6" max="-7,86,5"/>
        </regions>
    </spawn>
    <!-- Blue Team -->
    <spawn team="blue" kit="blue-kit">
        <regions yaw="90">
            <cuboid min="6,84,-6" max="8,86,5"/>
        </regions>
    </spawn>
    <!-- Obs (Spectators) Team -->
    <default>
        <regions>
            <point>-0.5,139,-0.5</point>
        </regions>
    </default>
</spawns>
<!-- Filters -->
<filters>
    <!-- Block filters -->
    <any id="white-clay">
        <material>stained clay:0</material>
    </any>
    <any id="black-clay">
        <material>stained clay:15</material>
    </any>
    <any id="blue-clay">
        <material>stained clay:11</material>
    </any>
    <any id="red-clay">
        <material>stained clay:14</material>
    </any>
    <any id="white-glass">
        <material>stained glass:0</material>
    </any>
    <any id="black-glass">
        <material>stained glass:15</material>
    </any>
    <any id="blue-glass">
        <material>stained glass:11</material>
    </any>
    <any id="red-glass">
        <material>stained glass:14</material>
    </any>
    <!-- First Platform -->
    <after id="first-white-clay-to-white-glass" duration="20s">
        <match-started/>
    </after>
    <after id="first-blue-clay-to-blue-glass" duration="20s">
        <match-started/>
    </after>
    <after id="first-red-clay-to-red-glass" duration="20s">
        <match-started/>
    </after>
    <after id="first-black-clay-to-black-glass" duration="20s">
        <match-started/>
    </after>
    <after id="first-white-glass-to-air" duration="30s">
        <match-started/>
    </after>
    <after id="first-blue-glass-to-air" duration="30s">
        <match-started/>
    </after>
    <after id="first-red-glass-to-air" duration="30s">
        <match-started/>
    </after>
    <after id="first-black-glass-to-air" duration="30s">
        <match-started/>
    </after>
    <!-- Second Platform -->
    <after id="second-white-clay-to-white-glass" duration="50s">
        <match-started/>
    </after>
    <after id="second-black-clay-to-black-glass" duration="50s">
        <match-started/>
    </after>
    <after id="second-white-glass-to-air" duration="60s">
        <match-started/>
    </after>
    <after id="second-black-glass-to-air" duration="60s">
        <match-started/>
    </after>
    <!-- Third Platform -->
    <after id="third-white-clay-to-white-glass" duration="80s">
        <match-started/>
    </after>
    <after id="third-blue-clay-to-blue-glass" duration="80s">
        <match-started/>
    </after>
    <after id="third-red-clay-to-red-glass" duration="80s">
        <match-started/>
    </after>
    <after id="third-black-clay-to-black-glass" duration="80s">
        <match-started/>
    </after>
    <after id="third-white-glass-to-air" duration="90s">
        <match-started/>
    </after>
    <after id="third-blue-glass-to-air" duration="90s">
        <match-started/>
    </after>
    <after id="third-red-glass-to-air" duration="90s">
        <match-started/>
    </after>
    <after id="third-black-glass-to-air" duration="90s">
        <match-started/>
    </after>
</filters>
<!-- Actions -->
<actions>
    <!-- First Platform -->
    <trigger scope="match" filter="first-white-clay-to-white-glass">
        <action>
            <fill region="first-platform" filter="white-clay" material="stained glass:0"/>
            <message text="`cWatch out! The top platform is breaking!"/>
            <sound preset="tip"/>
        </action>
    </trigger>
    <trigger scope="match" filter="first-blue-clay-to-blue-glass">
        <action>
            <fill region="first-platform" filter="blue-clay" material="stained glass:11"/>
        </action>
    </trigger>
    <trigger scope="match" filter="first-red-clay-to-red-glass">
        <action>
            <fill region="first-platform" filter="red-clay" material="stained glass:14"/>
        </action>
    </trigger>
    <trigger scope="match" filter="first-black-clay-to-black-glass">
        <action>
            <fill region="first-platform" filter="black-clay" material="stained glass:15"/>
        </action>
    </trigger>
    <trigger scope="match" filter="first-white-glass-to-air">
        <action>
            <fill region="first-platform" filter="white-glass" material="air"/>
        </action>
    </trigger>
    <trigger scope="match" filter="first-blue-glass-to-air">
        <action>
            <fill region="first-platform" filter="blue-glass" material="air"/>
        </action>
    </trigger>
    <trigger scope="match" filter="first-red-glass-to-air">
        <action>
            <fill region="first-platform" filter="red-glass" material="air"/>
        </action>
    </trigger>
    <trigger scope="match" filter="first-black-glass-to-air">
        <action>
            <fill region="first-platform" filter="black-glass" material="air"/>
            <message text="`cThe top platform has crumbled!"/>
            <sound preset="alert"/>
        </action>
    </trigger>
    <!-- Second Platform -->
    <trigger scope="match" filter="second-white-clay-to-white-glass">
        <action>
            <fill region="second-platform" filter="white-clay" material="stained glass:0"/>
            <message text="`cWatch out! The second platform is breaking!"/>
            <sound preset="tip"/>
        </action>
    </trigger>
    <trigger scope="match" filter="second-black-clay-to-black-glass">
        <action>
            <fill region="second-platform" filter="black-clay" material="stained glass:15"/>
        </action>
    </trigger>
    <trigger scope="match" filter="second-white-glass-to-air">
        <action>
            <fill region="second-platform" filter="white-glass" material="air"/>
        </action>
    </trigger>
    <trigger scope="match" filter="second-black-glass-to-air">
        <action>
            <fill region="second-platform" filter="black-glass" material="air"/>
            <message text="`cThe second platform has crumbled!"/>
            <sound preset="alert"/>
        </action>
    </trigger>
    <!-- Third Platform -->
    <trigger scope="match" filter="third-white-clay-to-white-glass">
        <action>
            <fill region="third-platform" filter="white-clay" material="stained glass:0"/>
            <message text="`cWatch out! The third platform is breaking!"/>
            <sound preset="tip"/>
        </action>
    </trigger>
    <trigger scope="match" filter="third-blue-clay-to-blue-glass">
        <action>
            <fill region="third-platform" filter="blue-clay" material="stained glass:11"/>
        </action>
    </trigger>
    <trigger scope="match" filter="third-red-clay-to-red-glass">
        <action>
            <fill region="third-platform" filter="red-clay" material="stained glass:14"/>
        </action>
    </trigger>
    <trigger scope="match" filter="third-black-clay-to-black-glass">
        <action>
            <fill region="third-platform" filter="black-clay" material="stained glass:15"/>
        </action>
    </trigger>
    <trigger scope="match" filter="third-white-glass-to-air">
        <action>
            <fill region="third-platform" filter="white-glass" material="air"/>
        </action>
    </trigger>
    <trigger scope="match" filter="third-blue-glass-to-air">
        <action>
            <fill region="third-platform" filter="blue-glass" material="air"/>
        </action>
    </trigger>
    <trigger scope="match" filter="third-red-glass-to-air">
        <action>
            <fill region="third-platform" filter="red-glass" material="air"/>
        </action>
    </trigger>
    <trigger scope="match" filter="third-black-glass-to-air">
        <action>
            <fill region="third-platform" filter="black-glass" material="air"/>
            <message text="`cThe third platform has crumbled!"/>
            <sound preset="alert"/>
        </action>
    </trigger>
</actions>
<!-- Regions -->
<regions>
    <!-- Definitions -->
    <cuboid id="first-platform" min="-11,80,-11" max="10,85,10"/>
    <cuboid id="second-platform" min="-25,65,-25" max="24,70,24"/>
    <cuboid id="third-platform" min="-11,37,-11" max="10,42,10"/>
    <below id="void-area" y="20"/>
    <!-- Applications -->
    <apply block="never" message="You are not allowed to modify terrain here."/>
    <apply region="void-area" kit="void-kit"/>
</regions>
<!-- World Border -->
<world-borders center="-0.5, -0.5">
    <world-border size="250"/>
    <world-border size="3" after="0s" duration="125s" damage="100" buffer="0"/>
</world-borders>
<!-- Itemremove -->
<itemremove>
    <item>ink sack</item>
    <item>leather helmet</item>
</itemremove>
<!-- Miscellaneous -->
<hunger>
    <depletion>off</depletion>
</hunger>
<disabledamage>
    <damage>fall</damage>
</disabledamage>
</map>